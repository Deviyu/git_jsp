<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>jsp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>ADMIN</param-name>
    <param-value>brown</param-value>
  </context-param>
  <servlet>
    <servlet-name>basicServlet</servlet-name>
    <servlet-class>kr.or.ddit.servlet.BasicServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>basicServlet</servlet-name>
    <url-pattern>/bs</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>nowDate</servlet-name>
    <servlet-class>kr.or.ddit.servlet.NowDate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>nowDate</servlet-name>
    <url-pattern>/nowDate</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TimesTablesServlet</servlet-name>
    <servlet-class>kr.or.ddit.servlet.TimesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TimesTablesServlet</servlet-name>
    <url-pattern>/TimesTablesServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LoginController</display-name>
    <servlet-name>LoginController</servlet-name>
    <servlet-class>kr.or.ddit.login.controller.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginController</servlet-name>
    <url-pattern>/LoginController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SumCalculation</display-name>
    <servlet-name>SumCalculation</servlet-name>
    <servlet-class>kr.or.ddit.servlet.SumCalculation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SumCalculation</servlet-name>
    <url-pattern>/SumCalculation</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MulCalculation</display-name>
    <servlet-name>MulCalculation</servlet-name>
    <servlet-class>kr.or.ddit.servlet.MulCalculation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MulCalculation</servlet-name>
    <url-pattern>/MulCalculation</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>UserList</display-name>
    <servlet-name>UserList</servlet-name>
    <servlet-class>kr.or.ddit.user.controller.UserList</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserList</servlet-name>
    <url-pattern>/UserList</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LogoutController</display-name>
    <servlet-name>LogoutController</servlet-name>
    <servlet-class>kr.or.ddit.login.controller.LogoutController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LogoutController</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/error/404/404.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/error/500/500.html</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.NumberFormatException</exception-type>
    <location>/WEB-INF/error/numberFormatException.jsp</location>
  </error-page>
  <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc/oracleDB</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <filter>
    <filter-name>loggingFilter</filter-name>
    <filter-class>kr.or.ddit.filter.LoggingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loggingFilter</filter-name>
    <url-pattern>*</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>loggingFilter2</filter-name>
    <filter-class>kr.or.ddit.filter.LoggingFilter2</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loggingFilter2</filter-name>
    <url-pattern>*</url-pattern>
  </filter-mapping>
</web-app>