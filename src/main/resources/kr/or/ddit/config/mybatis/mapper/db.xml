<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="db">
	<select id="getDb" parameterType="String" resultType="DbVO">
		SELECT	*
		FROM	URIMAPPING
		WHERE	URI = ${uri}
	</select>
	<select id="getDbList" resultType="DbVO">
		SELECT	*
		FROM	URIMAPPING
	</select>  
</mapper>